<Window x:Class="CodeGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CodeGenerator"
        mc:Ignorable="d"
        Title="MainWindow" Height="157" Width="189">
    <Grid>
        <Label x:Name="label" Content="Port Letters" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="70" Height="45"/>
        <Label x:Name="label1" Content="No of pins&#xD;&#xA;" HorizontalAlignment="Left" Margin="15,45,0,0" VerticalAlignment="Top" Height="25"/>
        <Button x:Name="button" Content="Generate" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top" Width="153" Click="button_Click"/>
        <TextBox x:Name="Letters" HorizontalAlignment="Left" Height="20" Margin="85,14,0,0" TextWrapping="Wrap" Text="ABCD" VerticalAlignment="Top" Width="80"/>
        <TextBox x:Name="Pins" HorizontalAlignment="Left" Height="23" Margin="80,47,0,0" TextWrapping="Wrap" Text="8" VerticalAlignment="Top" Width="83"/>

    </Grid>
</Window>
